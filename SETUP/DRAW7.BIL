Text1=CORELDRAW...\nPRODUTIVIDADE, POTÊNCIA E PRECISÃO
Font1=MS Sans Serif
Charset1=0
Color1=0,0,0
DropShadow=160,160,50,1
Frame1=15,15,465,345
